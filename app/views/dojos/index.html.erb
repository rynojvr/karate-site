<p id="notice"><%= notice %></p>

<h1>Dojos</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Cell</th>
      <th>Email</th>
      <th>Sensei</th>
      <th>Contact number</th>
      <th>Cell</th>
      <th>Landline</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @dojos.each do |dojo| %>
      <tr>
        <td><%= dojo.name %></td>
        <td><%= dojo.cell %></td>
        <td><%= dojo.email %></td>
        <td><%= dojo.sensei %></td>
        <td><%= dojo.contact_number %></td>
        <td><%= dojo.cell %></td>
        <td><%= dojo.landline %></td>
        <td><%= link_to 'Show', dojo %></td>
        <td><%= link_to 'Edit', edit_dojo_path(dojo) %></td>
        <td><%= link_to 'Destroy', dojo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Dojo', new_dojo_path %>
